{
  "ConnectionStrings": {
    "DefaultConnection": "[Insert your connection string in Secrets.json]"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "PollsAppBlazor.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "Auth": {
    "AccessTokenExpiryMinutes": 1,
    "RefreshTokenExpiryDays": 30
  },
  "AllowedHosts": "*"
}
